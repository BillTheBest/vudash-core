    "test": "lab -I describe,it,fromSrc,fromRoot,expect,beforeEach,afterEach,before,after,resource test/unit.lab.js"
  },
  "bin": {
    "vudash": "bin/vudash.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vudash/vudash-core"
  },
  "author": "Antony Jones",
  "license": "MIT",
  "dependencies": {
    "app-module-path": "^1.1.0",
    "bluebird": "^3.4.0",
    "boom": "^3.1.3",
    "browser-sync": "^2.12.9",
    "chalk": "^1.1.3",
    "cli": "^0.11.2",
    "flexboxgrid": "^6.3.0",
    "fs-extra": "^0.30.0",
    "handlebars": "^4.0.5",
    "hapi": "^13.4.0",
    "hoek": "^4.0.0",
    "inert": "^4.0.0",
    "joi": "^8.1.0",
    "jqlite": "^0.2.37",
    "jquery": "^2.2.4",
    "moment": "^2.13.0",
    "npm": "^3.9.3",
    "request": "^2.72.0",
    "semantic-ui-css": "^2.1.8",
    "shortid": "^2.2.6",
    "socket.io": "^1.4.6",
    "vision": "^4.1.0",
    "vudash-widget-pluck": "^1.0.1",
    "vudash-widget-time": "^1.0.0",
    "vudash-widget-travis": "^1.0.0"
  },
  "devDependencies": {
    "code": "^2.3.0",
    "lab": "^10.6.0",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "standard": "^7.1.2"
  },
  "standard": {
    "globals": [
      "fromSrc",
      "describe",
      "it",
      "fromRoot",
      "expect",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "resource"
